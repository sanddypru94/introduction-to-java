public class Solution {

	public static int findDuplicate(int[] arr) {
		//Your code goes here
        int s = 0;
        for(int i =0;i <(arr.length - 1); i++){
            s = s+i;
        }
        int se =0;
        for( int i =0; i< arr.length; i++){
            se = se+ arr[i];
        }
        return(se-s);
	}
}
